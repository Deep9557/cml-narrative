{
      "psi-header.templates": [

    {
      "language": "*",
      "template": [
        "<<filerelativepath>>"
        "<<filename>>",
        "<<projectName>>",
        "",
        "Created by <<author>> on <<filecreated('MMMM Do YYYY')>>",
        "<<authorEmail>>",
        "",
        "https://idealab.sites.clemson.edu",
        ""
      ]
    }
  ],
"psi-header.config": {
    "author": "Ian Thompson",
    "authorEmail": "icthomp@g.clemson.edu",
    "company": "IDEA Lab @ Clemson University",
  },
  "psi-header.lang-config": [

    {
      "language": "javascript",
      "begin": "/**",
      "prefix": " ",
      "end": "*/",
      "blankLinesAfter": 1,
      "forceToTop": false
    },
    {
      "language": "html",
      "begin": "<!--",
      "prefix": " ",
      "end": "--->",
      "blankLinesAfter": 1
    },
    {
      "language": "typescript",
      "mapTo": "javascript"
    }, 
    {
      "language": "svelte",
      "mapTo": "html"
    }
  ],
  "psi-header.changes-tracking": {
    "autoHeader": "autoSave"
  },
}